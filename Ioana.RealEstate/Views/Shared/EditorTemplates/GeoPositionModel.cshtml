@model GeoPositionModel

<div class="gmaps-editor">
    <div class="form-group">
        <div class="col-sm-6">
            @Html.TextBoxFor(m => m.Latitude, new { @class = "form-control lat-editor", placeholder = Resources.LatitudeLabel, @type = "number" })
        </div>
        <div class="col-sm-6">
            @Html.TextBoxFor(m => m.Longitude, new { @class = "form-control lon-editor", placeholder = Resources.LongitudeLabel, @type = "number" })
        </div>
    </div>
    <div class="gmap form-control">
    </div>
</div>
